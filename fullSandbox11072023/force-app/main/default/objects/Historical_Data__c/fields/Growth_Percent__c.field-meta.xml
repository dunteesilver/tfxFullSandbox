<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Growth_Percent__c</fullName>
    <description>The percent of growth between current year amount ans previous year amount</description>
    <externalId>false</externalId>
    <formula>IF(Previous_Year_Amount__c  = 0, 0,((Current_Year_Amount__c  -  Previous_Year_Amount__c )  /  Previous_Year_Amount__c  ))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Growth %</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>

<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>X2H_Current_Fiscal_Month_Sales__c</fullName>
    <description>Created to support Surgical H2 calendar</description>
    <externalId>false</externalId>
    <formula>AND(
MONTH(Date__c) = X2H_Current_Fiscal_Month_Number__c ,
YEAR(Date__c) = YEAR(TODAY())
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>2H Current Fiscal Month Sales</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>

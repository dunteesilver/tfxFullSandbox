<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Email_Cannot_Be_Blank</fullName>
    <active>false</active>
    <description>Enforce email address for Contacts</description>
    <errorConditionFormula>ISBLANK(Email)</errorConditionFormula>
    <errorDisplayField>Email</errorDisplayField>
    <errorMessage>Email address cannot be blank.</errorMessage>
</ValidationRule>

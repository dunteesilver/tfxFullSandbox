<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Return_Tracking_Number</fullName>
    <active>false</active>
    <errorConditionFormula>AND(OR(ispickval(Status__c,&quot;Transferred&quot;),ispickval(Status__c,&quot;Returned&quot;)),Return_Tracking_Number__c == null)</errorConditionFormula>
    <errorDisplayField>Return_Tracking_Number__c</errorDisplayField>
    <errorMessage>Return/Transfer Tracking Number Required</errorMessage>
</ValidationRule>

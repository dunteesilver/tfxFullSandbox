<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Was_the_difficulty_encountered_is_Blank</fullName>
    <active>true</active>
    <description>When was the difficulty encountered? Null check</description>
    <errorConditionFormula>AND(RecordType.Name = &quot;Regional Anesthesia&quot;,
ISBLANK(When_was_the_difficulty_encountered__c)
)</errorConditionFormula>
    <errorDisplayField>When_was_the_difficulty_encountered__c</errorDisplayField>
    <errorMessage>Please fill in &quot;When was the difficulty encountered?&quot; field to save this record</errorMessage>
</ValidationRule>

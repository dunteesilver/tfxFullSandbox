<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VPS_ConsolesStylets_Required_Field_6</fullName>
    <active>true</active>
    <errorConditionFormula>AND (
ISPICKVAL(  Is_this_complaint_regarding_VPS_Consoles__c , &quot;Yes&quot;),
ISBLANK(TEXT((      Was_the_catheter_placed_without_VPS__c     )))
)</errorConditionFormula>
    <errorDisplayField>Was_the_catheter_placed_without_VPS__c</errorDisplayField>
    <errorMessage>This field is required when the complaint is regarding VPS Consoles/Stylets</errorMessage>
</ValidationRule>

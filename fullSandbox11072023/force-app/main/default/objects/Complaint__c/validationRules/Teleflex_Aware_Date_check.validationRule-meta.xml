<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Teleflex_Aware_Date_check</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Teleflex_Aware_Date__c &lt;Event_Date__c ||Teleflex_Aware_Date__c &lt;	Estimated_Event_Date__c)</errorConditionFormula>
    <errorDisplayField>Teleflex_Aware_Date__c</errorDisplayField>
    <errorMessage>Teleflex Aware Date cannot be earlier than the Event date or Estimated Event Date</errorMessage>
</ValidationRule>

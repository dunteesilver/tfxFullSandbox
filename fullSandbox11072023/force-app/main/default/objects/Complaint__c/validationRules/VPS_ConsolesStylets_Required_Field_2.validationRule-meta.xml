<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VPS_ConsolesStylets_Required_Field_2</fullName>
    <active>true</active>
    <errorConditionFormula>AND (
ISPICKVAL(  Is_this_complaint_regarding_VPS_Consoles__c , &quot;Yes&quot;),
ISPICKVAL(Was_an_X_ray_performed__c,&quot;Yes&quot;),
ISPICKVAL(   Is_X_ray_available_for_review__c ,&apos;&apos; ))</errorConditionFormula>
    <errorDisplayField>Is_X_ray_available_for_review__c</errorDisplayField>
    <errorMessage>This field is required when the complaint is regarding VPS Consoles/Stylets</errorMessage>
</ValidationRule>

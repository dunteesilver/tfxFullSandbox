<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Sales_and_Primary_Email__c</fullName>
    <externalId>false</externalId>
    <formula>AND(
NOT(ISBLANK(Sales_Rep_Email__c)),
NOT(ISBLANK(Primary_Territory_User_Email__c)),
Sales_Rep_Email__c = Primary_Territory_User_Email__c

)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Checked if Sales Rep and Primary Sales Rep email addresses match</inlineHelpText>
    <label>Check Sales and Primary Email</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>

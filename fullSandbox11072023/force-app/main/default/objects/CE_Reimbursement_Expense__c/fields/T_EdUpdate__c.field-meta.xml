<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>T_EdUpdate__c</fullName>
    <description>used to calculate Total CE Reimbursement expenses</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Education_Record__r.Primary_Product__c, &quot;EZ-IO&quot;),
(Ed_CE__c*25.00),
(Ed_CE__c * C_Rate__c /2))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total EdUpdate for CE calc</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>

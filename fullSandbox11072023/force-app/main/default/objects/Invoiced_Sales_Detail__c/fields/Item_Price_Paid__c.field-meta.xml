<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Item_Price_Paid__c</fullName>
    <description>Formula Field created to show price paid by the customer for each item</description>
    <externalId>false</externalId>
    <formula>(Sales__c / Quantity__c ) * Product__r.Qty_Sales_Unit__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Item Price Paid</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>

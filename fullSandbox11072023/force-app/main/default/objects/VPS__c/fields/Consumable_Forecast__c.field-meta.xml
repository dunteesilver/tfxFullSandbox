<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Consumable_Forecast__c</fullName>
    <description>Consumable Spend/Consumable Attainment Days = Daily Spend*30 = monthly spend</description>
    <externalId>false</externalId>
    <formula>(Consumable_Spend__c/Consumable_Attainment__c)*Consumable_Attainment_Days_Left__c+Consumable_Spend__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Amount customer will spend for 12 month period using their previous purchases for run rate.</inlineHelpText>
    <label>Consumable Forecast</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>

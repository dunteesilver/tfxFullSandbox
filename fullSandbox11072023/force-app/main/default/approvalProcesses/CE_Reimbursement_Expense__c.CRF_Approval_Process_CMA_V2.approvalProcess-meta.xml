<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>creator</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Teleflex_Global_IT</submitter>
        <type>role</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Education_Record__c</field>
        <field>Grand_Total_Reimbursement__c</field>
        <field>Education_Contractor__c</field>
        <field>Engagement_Type__c</field>
        <field>Clinical_Educator_Name__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>beth.conaway@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>heather.wear@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>CMA Contractor</label>
        <name>CMA_Contractor</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>heather.suhor@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>jim.blosser@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Clinical Coordinator</label>
        <name>Step_Jim</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>dan.smith@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>kelli.thomas@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Final Approval</label>
        <name>Final_Approval</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <description>v2</description>
    <emailTemplate>Workflow_Emails/CRF_Supa</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>CE_Reimbursement_Expense__c.Status__c</field>
            <operation>notEqual</operation>
            <value>Submit CRF</value>
        </criteriaItems>
        <criteriaItems>
            <field>Education_Record__c.RecordType</field>
            <operation>equals</operation>
            <value>CMA Procedural Lab Request,CMA Speaking Engagement Request,CMA Record,CMA Record MEG</value>
        </criteriaItems>
        <criteriaItems>
            <field>Education_Record__c.Business_Unit__c</field>
            <operation>notEqual</operation>
            <value>IA</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>CE_Approved_Email_to_CE</name>
            <type>Alert</type>
        </action>
        <action>
            <name>CRF_Submit_Date</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>CRF_Submitted_by</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Status_to_Submit_CRF</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>CE_Rejected_Email_to_CE</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Changed_to_New</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>CE_Expenses_Form_submission_send_to_CE</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Status_to_Pending</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>CRF Approval  Process CMA</label>
    <processOrder>2</processOrder>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>

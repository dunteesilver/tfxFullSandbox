<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>creator</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Commercial_Operations</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Director_Corporate_Accounts_Canada</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Respiratory_President_Corporate_Accounts_VP</submitter>
        <type>role</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>Account_Name__c</field>
        <field>Commit_Date__c</field>
        <field>Expiration_Date__c</field>
        <field>Status__c</field>
        <field>Internal_Comments_Only__c</field>
        <field>Customer_comments__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt;  Approval_Level__c, Owner_Approval_Level__c &lt; 2 )</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>RSM</label>
        <name>RSM</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Contracts_Team</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt; 3, Approval_Level__c &gt;=3 )</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Contracts 1st Approval</label>
        <name>Contracts_1st_Approval</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>CC_Below_Director_Level_Appointed_Person</name>
                <type>Alert</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>emily.sizemore@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>richard.folk@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt; 4, Approval_Level__c &gt;=3, Owner:User.Manager.Approval_Level__c &lt; 4)</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Anesthesia Below RSM Level</label>
        <name>Anesthesia_Below_RSM_Level</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>brad.libertore@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>sean.higgins@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt; 4, Approval_Level__c &gt;=3, Owner:User.Manager.Approval_Level__c &lt; 4)</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Anesthesia Below RSM Level 1</label>
        <name>Anesthesia_Below_RSM_Level_1</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>arthur.fragakis@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>dave.price1@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>don.mcintosh@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <description>Added to handle and condition in Below RSM Level.</description>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt; 4, Approval_Level__c &gt;=3, Owner:User.Manager.Approval_Level__c &lt; 4)</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Anesthesia Below RSM Level 2</label>
        <name>Anesthesia_Below_RSM_Level_2</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>brad.libertore@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>mark.reis@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt;= 4, Approval_Level__c = 4 )</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Anesthesia Below Director Level</label>
        <name>Anesthesia_Below_Director_Level</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Contracts_Team</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Contracts Final Approval</label>
        <name>Contracts_Final_Approval</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <description>Approval Matrix Process for Anesthesia SBU</description>
    <emailTemplate>Pricing_Portal/Approval_Price_Quote_Supa</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>AND( Line_Count__c  &gt; 0,  NOT(ISPICKVAL(Status__c, &apos;Approved&apos;)),  BusinessUnit__c = &apos;Anesthesia&apos;)</formula>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Completed_Approval_Process</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>PP_Approval_Notification</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Update_Commit_Date</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Update_status_to_Approved</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Price_Quote_Rejection_Email</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Update_status_to_Rejected</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Waiting_on_Approval</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>PQ Approval Anesthesia v1.2</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>true</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <processOrder>5</processOrder>
    <recallActions>
        <action>
            <name>Update_Status_to_In_Progress</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>

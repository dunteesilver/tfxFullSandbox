    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>Account__c</field>
        <field>Agreement_Type__c</field>
        <field>Current_Annual_Purchases__c</field>
        <field>Eligible_Distributor__c</field>

            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt;= 3, Approval_Level__c &gt;= 3, OR(Business_Unit__c = &apos;Alternate Care&apos;, Business_Unit__c = &apos;Urology&apos;) )</formula>

        <field>Territory__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt;= Approval_Level__c, Owner_Approval_Level__c &lt; 2)</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>RSM</label>
        <name>RSM</name>
        <rejectionActions>
            <action>
                <name>Update_to_Rejected</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>brad.libertore@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt; 4, Approval_Level__c &gt;=3, Business_Unit__c = &apos;Anesthesia&apos; )</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Below RSM Level Anesthesia</label>
        <name>Below_RSM_Level_Anesthesia</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>

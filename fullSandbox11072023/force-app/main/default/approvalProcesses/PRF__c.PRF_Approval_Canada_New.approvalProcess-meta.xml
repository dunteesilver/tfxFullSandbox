<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>creator</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Commercial_Operations</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Director_Corporate_Accounts_Canada</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Respiratory_President_Corporate_Accounts_VP</submitter>
        <type>role</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>Account__c</field>
        <field>Agreement_Type__c</field>
        <field>Current_Annual_Purchases__c</field>
        <field>Eligible_Distributor__c</field>
        <field>Estimated_Annual_Purchases__c</field>
        <field>Requisition_Date__c</field>
        <field>Sales_Org_Key__c</field>
        <field>Territory__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt;= Approval_Level__c, Owner_Approval_Level__c &lt; 2 )</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>RSM</label>
        <name>RSM</name>
        <rejectionActions>
            <action>
                <name>Update_to_Rejected</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &gt; Approval_Level__c, Owner_Approval_Level__c &lt; 3, Approval_Level__c &gt;=3 )</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Below RSM Level</label>
        <name>Below_RSM_Level</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>arthur.fragakis@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>dave.price1@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt; 4, Approval_Level__c = 4, Owner_Business_Unit__c = &apos;Anesthesia&apos; )</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Anesthesia Below Cost Level</label>
        <name>Anesthesia_Below_Cost_Level</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>dave.price1@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>james.wysocki@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt; 4, Approval_Level__c = 4, OR(Owner_Business_Unit__c = &apos;Alternate Care&apos;, Owner_Business_Unit__c = &apos;Urology&apos; ) )</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Alternate Care/Urology Below Cost</label>
        <name>Alternate_Care_Urology_Below_Cost</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>dave.price1@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>mark.singleton@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt; 4, Approval_Level__c = 4, Owner_Business_Unit__c = &apos;Vascular&apos; )</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Vascular Below Cost Level</label>
        <name>Vascular_Below_Cost_Level</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>dave.price1@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>jennifer.dallaire@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt; 4, Approval_Level__c = 4,  CONTAINS( Owner_Business_Unit__c , &apos;Interventional&apos;)  
)</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Interventional Below Cost</label>
        <name>Interventional_Below_Cost</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>dave.price1@teleflex.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>sue.clarke@teleflex.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>AND(Owner_Approval_Level__c &lt; Approval_Level__c, Owner_Approval_Level__c &lt; 4, Approval_Level__c = 4, Owner_Business_Unit__c = &apos;Surgical&apos; )</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Surgical Below Cost Level</label>
        <name>Surgical_Below_Cost_Level</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Contracts_Team</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Contracts Final Approval</label>
        <name>Contracts_Final_Approval</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <emailTemplate>Pricing_Portal/Approval_PRF_Supa</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>AND( Line_Count__c  &gt; 0,  NOT(ISPICKVAL(Status__c, &apos;Approved&apos;)), ISPICKVAL(Owner:User.Region__c, &apos;Canada&apos;) )</formula>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>PRF_Approved_Email</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Update_to_Approved</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>PRF_Rejected_Email</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Update_to_Rejected</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Waiting_on_Approval</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>PRF Approval Canada New</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>true</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <processOrder>1</processOrder>
    <recallActions>
        <action>
            <name>Change_Status_to_New</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>

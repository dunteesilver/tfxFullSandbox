    <rules>
        <fullName>Imperative - Update Task Subject</fullName>
        <actions>
            <name>Imperative_Update_Task_Subject</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(
RecordType.Name = &quot;Imperative Task&quot;,
Subject &lt;&gt; TEXT(Type__c)
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Notification of A5 2-L Catheter Cross-Se</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Task.Subject</field>
            <operation>contains</operation>
            <value>A5 2-L Catheter</value>
        </criteriaItems>
        <criteriaItems>
            <field>Task.Status</field>
            <operation>notEqual</operation>
            <value>Completed</value>
        </criteriaItems>
        <description>Email Alert when task is not completed within a certain time frame</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Notification_of_A5_2_L_Catheter_Cross_Section_Change_30_Day_Reminder</name>

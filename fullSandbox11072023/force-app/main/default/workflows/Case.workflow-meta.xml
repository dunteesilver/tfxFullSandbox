    <rules>
        <fullName>CASE%3A  UPDATE RECORD TYPE TO CM</fullName>
        <active>false</active>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>Case.Type</field>
            <operation>equals</operation>
            <value>Feature Request</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Reason</field>
            <operation>equals</operation>
            <value>Functionality not working properly,New business requirement</value>
        </criteriaItems>
        <description>Updates the record type to Change Management whenever a new business requirement is submitted.</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>

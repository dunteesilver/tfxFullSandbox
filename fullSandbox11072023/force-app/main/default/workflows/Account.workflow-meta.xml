    <rules>
        <fullName>Account - Update Billing Address</fullName>
        <actions>
            <name>Account_Update_Billing_City</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Account_Update_Billing_Country</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Account_Update_Billing_State</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Account_Update_Billing_Street</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Account_Update_Billing_Zip</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND( 
      AND( 
            (Account_Record_Type_Name__c &lt;&gt; &quot;Prospect Customer&quot;), 
            (Account_Record_Type_Name__c &lt;&gt; &quot;Associations&quot;)), 
      OR( 
            BillingCity &lt;&gt; ShippingCity, 
            BillingCountry &lt;&gt; ShippingCountry, 
            BillingState &lt;&gt; ShippingState, 
            BillingPostalCode &lt;&gt; ShippingPostalCode, 
            BillingStreet &lt;&gt; ShippingStreet 
))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Account data has changed</fullName>
        <actions>
            <name>Timestamp_ECENTA_Field</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Rule to check for certain changes in account data to send accounts over to ECENTA.</description>
        <formula>OR( ISCHANGED(BillingStreet),ISCHANGED(BillingCity),ISCHANGED(BillingState),ISCHANGED(BillingPostalCode),ISCHANGED(BillingCountry), ISCHANGED(ShippingStreet),ISCHANGED(ShippingCity),ISCHANGED(ShippingState),ISCHANGED(ShippingPostalCode),ISCHANGED(ShippingCountry), ISCHANGED( Type ), ISCHANGED( Main_GPO__c ), ISCHANGED( Secondary_GPO__c ), ISCHANGED( ERP_Active__c ), ISCHANGED( IDN__c ), ISCHANGED( Radiology__c ), ISCHANGED( Emergency__c ), ISCHANGED( Cath_Lab__c ) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Association Account - Update Shipping Address</fullName>
        <actions>
            <name>Account_Update_Shipping_City</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>

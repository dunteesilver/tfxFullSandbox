            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Account_Update_Shipping_Zip</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>AND(         Account_Record_Type_Name__c =  &quot;Associations&quot;,        NOT(ISBLANK(BillingAddress)),        ISBLANK(ShippingAddress))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>

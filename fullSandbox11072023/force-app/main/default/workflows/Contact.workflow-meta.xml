    <rules>
        <fullName>ANZ Contact Address Updates</fullName>
        <actions>
            <name>Account_City_Address_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Account_Country_Address_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Account_State_Address_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Account_Zip_Address_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Contact_Street_Address_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>OR(

 ISPICKVAL(CreatedBy.Business_Unit__c, &quot;Australia&quot;),

 ISPICKVAL(CreatedBy.Business_Unit__c, &quot;New Zealand&quot;),

ISPICKVAL( LastModifiedBy.Business_Unit__c , &quot;Australia&quot;),

ISPICKVAL(LastModifiedBy.Business_Unit__c, &quot;New Zealand&quot;) 

)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Cardiac Service Survey</fullName>
        <actions>
            <name>Cardiac_Service_Survey</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Date_Time_Cardiac_Survey_Sent</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Cardiac_Service_Survey_Sent</name>
            <type>Task</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Contact.Cardiac_Send_Survey__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Cardiac Survey - 7 Day Reminder</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Contact.Date_Time_Cardiac_Survey_Sent__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Date_Time_Cardiac_Survey_Returned__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <description>Sends an email after 7 days if Date/Time Cardiac Survey Sent is populated but Date/Time Cardiac Survey Returned is not.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Cardiac_Service_Survey</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>Cardiac_Survey_7_Day_Followup</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Contact.Date_Time_Cardiac_Survey_Sent__c</offsetFromField>
            <timeLength>7</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Qualtrics Interventional Survey WrkFlw</fullName>
        <actions>
            <name>Date_Time_Cardiac_Survey_Sent</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Qualtrics_Interventional_Survey_OutMsg</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Contact.Cardiac_Send_Survey__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Email</field>
            <operation>notContain</operation>
            <value>@teleflex.com</value>
        </criteriaItems>
        <description>This survey is triggered to send survey email when &quot;Cardiac Send Survey&quot; is checked on any contact card.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>

        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Probability</field>
            <operation>greaterOrEqual</operation>
            <value>70</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Product_Family_GPL__c</field>
            <operation>equals</operation>
            <value>Pain Management</value>
        </criteriaItems>
        <description>Alerts the Anesthesia marketing team that a pain management opportunity is at a high percentage of being won.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Anesthesia Update Focus Product Checkbox</fullName>
        <actions>
            <name>Check_Focus_Product_checkbox</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND 2</booleanFilter>
        <criteriaItems>
            <field>Opportunity.Business_Unit_GPL__c</field>
            <operation>equals</operation>
            <value>Anesthesia,EMS</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Product_of_Interest_GPL__c</field>
            <operation>equals</operation>
            <value>AirTraq (Avant &amp; SP),DispoGRIP,DispoLED,EquipLite,Green RuschLite,TruLite</value>
        </criteriaItems>
        <description>Workflow to check the Focus Product checkbox if the Product of Interest contains a specific list of products of interest for Anesthesia</description>
        <triggerType>onAllChanges</triggerType>
    </rules>

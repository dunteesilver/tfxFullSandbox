    <rules>
        <fullName>Anesthesia and Respiratory Opportunity Closed Won Notice</fullName>
        <actions>
            <name>Opportunity_Closed_Won_Notice</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <description>Email notice sent to the Anesthesia Manager and Respiratory Manager when Opportunity stage = closed won</description>
        <formula>AND(RecordTypeId= &quot;012L0000000DGUm&quot;, RecordTypeId= &quot;012L0000000DGUh&quot;, Owner.UserRole.Name =&quot;AR Sales NA MSR Rocky Mountain&quot;,(ISPICKVAL(StageName, &quot;Closed Won&quot;)))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Anesthesia and Respiratory Opportunity Closing Notice</fullName>
        <active>false</active>
        <description>Email notice sent to the Opportunity Owner of an Opportunity that is 7 days from the close date, the day of, and once per week after</description>
        <formula>AND( RecordTypeId = &quot;012L0000000DGUh&quot;, RecordTypeId = &quot;012L0000000DGUm&quot;, AND(  Owner_Region__c  &lt;&gt; &quot;Great Lakes&quot;, AND( Owner_Region__c &lt;&gt; &quot;Rocky Mountains&quot;, AND( IsClosed  = False))))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>X7_Day_After_Opportunity_Closing_Notice</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Opportunity.CloseDate</offsetFromField>
            <timeLength>7</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <offsetFromField>Opportunity.CloseDate</offsetFromField>
            <timeLength>-7</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>X1_Day_Before_Opportunity_Closing_Notice</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Opportunity.CloseDate</offsetFromField>
            <timeLength>-1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>

    <rules>
        <fullName>Anesthesia Airway Probability Alert</fullName>
        <actions>
            <name>A_R_Airway_Probability_Alert</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Opportunity.RecordTypeId</field>
            <operation>equals</operation>
            <value>Anesthesia Opportunity,Respiratory Opportunity</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Probability</field>
            <operation>greaterOrEqual</operation>
            <value>70</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Product_Family_GPL__c</field>
            <operation>equals</operation>
            <value>Airway</value>
        </criteriaItems>
        <description>Alerts the Anesthesia marketing team that a airway opportunity is at a high percentage of being won.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Anesthesia Pain Management Probability Alert</fullName>

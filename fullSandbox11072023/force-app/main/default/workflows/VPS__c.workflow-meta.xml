    <rules>
        <fullName>Check Checkbox if Expired</fullName>
        <actions>
            <name>Expired_True</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>VPS__c.Days_to_Expire__c</field>
            <operation>lessThan</operation>
            <value>0</value>
        </criteriaItems>
        <criteriaItems>
            <field>VPS__c.Remedied__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Contract Expiration Emails</fullName>
        <active>true</active>
        <criteriaItems>
            <field>VPS__c.Active__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>VPS__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>VPS Contracts</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>VPS_Contract_Expiration_30_Day_Reminder</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>X30_Day_Email_Sent</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>VPS__c.Loaner_Expiration_Date__c</offsetFromField>
            <timeLength>-30</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
